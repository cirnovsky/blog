{
  "version": "0.0.6",
  "license": "MIT",
  "keywords": [
    "ast",
    "html to ast"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byoungd/html-to-ast.git"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^9.0.0",
    "size-limit": "^7.0.5",
    "tsdx": "^0.13.3",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "webpack": "^5.89.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "html-to-ast",
  "author": "byoungd",
  "module": "dist/htmltoast.esm.js",
  "size-limit": [
    {
      "path": "dist/htmltoast.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/htmltoast.esm.js",
      "limit": "10 KB"
    }
  ],
  "exports": {
    ".": {
      "import": "./dist/html-to-ast.esm.js",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@jridgewell/trace-mapping": "^0.3.20"
  }
}
